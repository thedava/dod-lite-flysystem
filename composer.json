{
    "name": "thedava/dod-lite-flysystem",
    "description": "A flysystem adapter for dod-lite",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "league/flysystem": "^3.0",
        "thedava/dod-lite": "^0.0"
    },
    "autoload": {
        "psr-4": {
            "DodLite\\Extension\\Flysystem\\": "src/DodLite/Extension/Flysystem/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.28",
        "league/flysystem-memory": "^3.0"
    }
}
